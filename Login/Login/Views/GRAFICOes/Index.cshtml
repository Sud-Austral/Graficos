@model IEnumerable<Login.Models.GRAFICO>

@{
    ViewBag.Title = "Index";
}

    <h2>Index</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nombre)
            </th>
            @*
                <th>
                    @Html.DisplayNameFor(model => model.descripcion)
                </th>
            *@
            <th>
                @Html.DisplayNameFor(model => model.titulo)
            </th>
            <th>
                Ruta
            </th>

            <th>
                @Html.DisplayNameFor(model => model.url)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DETALLE.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FUENTE.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PARAMETRO.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RESPONSABLE1.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TEMPORALIDAD.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TERRITORIO.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TIPO_GRAFICO.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UNIDAD_MEDIDA1.nombre)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.nombre)
                </td>
                @*
                    <td>
                        @Html.DisplayFor(modelItem => item.descripcion)
                    </td>
                *@
                <td>
                    @Html.DisplayFor(modelItem => item.titulo)
                </td>
                <td>
                    @item.CATEGORIA.nombre/@item.CATEGORIA.PRODUCTO.nombre/@item.CATEGORIA.PRODUCTO.SECTOR.nombre/@item.CATEGORIA.PRODUCTO.SECTOR.INDUSTRIA.nombre
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.url)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DETALLE.nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FUENTE.nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PARAMETRO.nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RESPONSABLE1.nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TEMPORALIDAD.nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TERRITORIO.nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TIPO_GRAFICO.nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UNIDAD_MEDIDA1.nombre)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.id })
                </td>
            </tr>
        }

    </table>
